local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Dark is op", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local mainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = mainTab:AddSection({
	Name = "Section"
})


mainTab:AddButton({
	Name = "Huge games dark",
	Callback = function()
      		getgenv().AuthKey = "HUGE_3C4qcDEzpSth"
getgenv().LoadSettings = {
    Example_Setting = Example_Value
}
loadstring(game:HttpGet("https://HugeGames.io/ps99"))()
  	end    
})


mainTab:AddButton({
	Name = "Simple spy",
	Callback = function()
      		loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-RemoteSpy-For-Mobile-6609"))()
  	end    
})


mainTab:AddButton({
	Name = "fps v2 3d dissable",
	Callback = function()
      		local MAX_FPS = 360
local MIN_FPS = 5
local 3D_RENDER = true
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

UserInputService.WindowFocused:Connect(function()
   if 3D_RENDER then
       RunService:Set3dRenderingEnabled(true)
   end
   setfpscap(MAX_FPS)
end)

UserInputService.WindowFocusReleased:Connect(function()
   if 3D_RENDER then
       RunService:Set3dRenderingEnabled(false)
   end
   setfpscap(MIN_FPS)
end)
  	end    
})


mainTab:AddButton({
	Name = "Anti afk",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darks-scripts/anti-afk-/main/darkscripts"))()
  	end    
})


mainTab:AddButton({
	Name = "♾ infinity yeild ♾",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end    
})

mainTab:AddButton({
	Name = "♾ anti lag ♾",
	Callback = function()
      		game.Lighting.FogEnd = 9999999999 -- stops fog (annoying)
	wait(1)
	for i,v in pairs(game:GetService("Lighting"):GetChildren()) do
        if v:IsA("PostEffect") then
            v:Destroy()
        end
	    end
	wait(1)
	game.Lighting.Blur.Size = 0 -- stops blur (cancer)
  	end    
})


local darkTab = Window:MakeTab({
	Name = "Dark",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = darkTab:AddSection({
	Name = "Section"
})

darkTab:AddToggle({
	Name = "SecretKey_Combine",
	Default = false,
	Callback = function(Value)
		autoBreakSecretKey_Combine = Value
    while autoBreakSecretKey_Combine and task.wait() do
    game:GetService("ReplicatedStorage").Network.SecretKey_Combine:InvokeServer()
            end
	end    
})

darkTab:AddToggle({
	Name = "CrystalKey_Combine",
	Default = false,
	Callback = function(Value)
		autoBreakCrystalKey_Combine = Value
    while autoBreakCrystalKey_Combine and task.wait() do
    game:GetService("ReplicatedStorage").Network.CrystalKey_Combine:InvokeServer()
            end
	end    
})

darkTab:AddToggle({
	Name = "StarterSpinny Wheel",
	Default = false,
	Callback = function(Value)
		autoBreakSpinnyWheel = Value
    while autoBreakSpinnyWheel and task.wait() do
    local args = {
    [1] = "StarterWheel"
}

game:GetService("ReplicatedStorage").Network:FindFirstChild("Spinny Wheel: Request Spin"):InvokeServer(unpack(args))
            end
	end    
})

darkTab:AddToggle({
	Name = "CrystalKeychest",
	Default = false,
	Callback = function(Value)
		autoBreakCrystalKey_Unlock = Value
    while autoBreakCrystalKey_Unlock and task.wait() do
    game:GetService("ReplicatedStorage").Network.CrystalKey_Unlock:InvokeServer()
            end
	end    
})

darkTab:AddToggle({
	Name = "TNT_Crate_Consume",
	Default = false,
	Callback = function(Value)
		autoBreakTNT_Crate_Consume = Value
    while autoBreakTNT_Crate_Consume and task.wait() do
    game:GetService("ReplicatedStorage").Network.TNT_Crate_Consume:InvokeServer()
            end
	end    
})

darkTab:AddToggle({
	Name = "Open Charm Stone",
	Default = false,
	Callback = function(Value)
		autoBreakGiftBag_Open = Value
    while autoBreakGiftBag_Open and task.wait() do
    local args = {
    [1] = "Charm Stone"
}

game:GetService("ReplicatedStorage").Network.GiftBag_Open:InvokeServer(unpack(args))
            end
	end    
})

